{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "ordered-imports": [
      false
    ],
    "interface-name": [
      false
    ],
    "jsx-self-close": false,
    "member-access": [
      false
    ],
    "semicolon": [
      true,
      "never"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "quotemark": [
      true,
      "single"
    ],
    "no-unused-expression": false
  }
}